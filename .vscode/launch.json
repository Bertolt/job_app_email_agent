{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Default Run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/job_email_agent.py",
            "args": [],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Run with Test Data",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/job_email_agent.py",
            "args": [
                "--input_file", "${workspaceFolder}/agents/data/email_data/test_sample_messages_with_crlf.csv",
                "--output_file", "${workspaceFolder}/agents/data/email_data/test_output.csv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Run with Duplicated Data",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/job_email_agent.py",
            "args": [
                "--input_file", "${workspaceFolder}/agents/data/email_data/test_sample_duplicated_messages_with_crlf.csv",
                "--output_file", "${workspaceFolder}/agents/data/test_duplicated_output.csv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Run with Different Model",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/job_email_agent.py",
            "args": [
                "--model_name", "mistral"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Allow Duplicates (30 day window)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/job_email_agent.py",
            "args": [
                "--allow_duplicates", "yes",
                "--duplicates_window", "30"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Integration Test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/agents/examples/test_integration.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Run Unit Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "agents/tests",
                "-v"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}